{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "nextjs",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev --port 80",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "start": "pnpm with-env next start",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "prettier": "@vyductan/prettier-config",
  "eslintConfig": {
    "extends": [
      "@vyductan/eslint-config/base",
      "@vyductan/eslint-config/react",
      "@vyductan/eslint-config/nextjs",
      "@vyductan/eslint-config/tailwind"
    ],
    "root": true
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.17.0",
    "@headlessui/react": "^1.7.17",
    "@next/bundle-analyzer": "^14.1.0",
    "@notionhq/client": "^2.2.13",
    "@popperjs/core": "^2.11.8",
    "@react-stately/calendar": "^3.4.3",
    "@react-stately/datepicker": "^3.9.1",
    "@sendgrid/mail": "^8.1.0",
    "@stitches/react": "^1.2.8",
    "@t3-oss/env-nextjs": "^0.8.0",
    "@tanstack/react-query": "^5.18.1",
    "@trpc/client": "11.0.0-next-beta.264",
    "@trpc/next": "11.0.0-next-beta.264",
    "@trpc/react-query": "11.0.0-next-beta.264",
    "@vercel/blob": "^0.20.0",
    "@vercel/kv": "^1.0.1",
    "@vyductan/api": "workspace:*",
    "@vyductan/hooks": "workspace:*",
    "@vyductan/icons": "workspace:*",
    "@vyductan/tts": "workspace:*",
    "@vyductan/ui": "workspace:*",
    "@vyductan/ui-pro": "workspace:*",
    "@vyductan/utils": "workspace:*",
    "ai": "^2.2.33",
    "antd": "^5.12.5",
    "axios": "^1.6.2",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "eventsource-parser": "^1.1.1",
    "framer-motion": "^11.0.3",
    "geist": "^1.2.2",
    "lodash": "^4.17.21",
    "negotiator": "^0.6.3",
    "next": "14.1.0",
    "next-international": "^1.2.3",
    "next-mdx-remote": "^4.4.1",
    "next-redux-wrapper": "^8.1.0",
    "next-themes": "^0.2.1",
    "nodemailer": "^6.9.7",
    "nprogress": "^0.2.0",
    "openai": "^4.26.1",
    "openai-edge": "^1.2.2",
    "openai-ext": "^1.2.7",
    "react": "18.2.0",
    "react-cookie": "^7.0.2",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.50.0",
    "react-intersection-observer": "^9.7.0",
    "react-popper": "^2.3.0",
    "react-scroll": "^1.9.0",
    "react-wrap-balancer": "^1.1.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "superjson": "2.2.1",
    "typewriter-effect": "^2.21.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.16",
    "@types/nodemailer": "^6.4.14",
    "@types/nprogress": "^0.2.3",
    "@types/react": "18.2.54",
    "@types/react-scroll": "^1.8.10",
    "@vyductan/eslint-config": "workspace:*",
    "@vyductan/tailwind-config": "workspace:*",
    "@vyductan/typescript-config": "workspace:*",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.56.0",
    "postcss-replace": "^2.0.1",
    "sass": "^1.70.0",
    "typescript": "5.3.3"
  }
}
