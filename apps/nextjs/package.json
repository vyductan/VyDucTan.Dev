{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "nextjs",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "bun with-env next dev --port 8888",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "lint:fix": "pnpm lint --fix",
    "start": "pnpm with-env next start",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "prettier": "@vyductan/prettier-config",
  "eslintConfig": {
    "extends": [
      "@vyductan/eslint-config/base",
      "@vyductan/eslint-config/react",
      "@vyductan/eslint-config/nextjs",
      "@vyductan/eslint-config/tailwind"
    ],
    "root": true
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.16.1",
    "@headlessui/react": "^1.7.17",
    "@hookform/resolvers": "^3.3.1",
    "@next/bundle-analyzer": "^13.5.4",
    "@notionhq/client": "^2.2.13",
    "@popperjs/core": "^2.11.8",
    "@react-stately/calendar": "^3.4.1",
    "@react-stately/datepicker": "^3.8.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@sendgrid/mail": "^7.7.0",
    "@stitches/react": "^1.2.8",
    "@t3-oss/env-nextjs": "^0.7.0",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@trpc/client": "^10.40.0",
    "@trpc/next": "^10.40.0",
    "@trpc/react-query": "^10.40.0",
    "@trpc/server": "^10.40.0",
    "@vyductan/auth": "*",
    "@vyductan/db": "^0.1.0",
    "@vyductan/server-action": "^0.1.0",
    "@vyductan/ui": "^0.1.0",
    "antd": "^5.10.0",
    "axios": "^1.5.1",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "eventsource-parser": "^1.1.1",
    "firebase": "^10.4.0",
    "framer-motion": "^10.16.4",
    "google-auth-library": "^9.1.0",
    "google-translate-tts": "0.4.0-dev",
    "google-tts-api": "^2.0.2",
    "lodash": "^4.17.21",
    "next": "13.5.4",
    "next-compose-plugins": "^2.2.1",
    "next-mdx-remote": "^4.4.1",
    "next-redux-wrapper": "^8.1.0",
    "nodemailer": "^6.9.6",
    "nprogress": "^0.2.0",
    "openai": "^4.12.0",
    "openai-ext": "^1.2.7",
    "react": "18.2.0",
    "react-cookie": "^6.1.1",
    "react-dom": "18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.47.0",
    "react-markdown": "^9.0.0",
    "react-popper": "^2.3.0",
    "react-redux": "^8.1.3",
    "react-scroll": "^1.9.0",
    "react-syntax-highlighter": "^15.5.0",
